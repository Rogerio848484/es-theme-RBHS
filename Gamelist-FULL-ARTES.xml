<?xml version="1.0" encoding="UTF-8"?>
<theme>
<formatVersion>7</formatVersion>

<customView name="Gamelist Full Artes" inherits="detailed">	

<!------------------------------------------------------------------------------->
<!---------------------------------- GAMELIST ----------------------------------->
<!------------------------------------------------------------------------------->
   
<textlist name="gamelist">
<!--  Tamanho da caixa das letras  -->		
    <size>0.2750 0.92</size>
<!--  Fonte das letras  -->	
    <fontPath>./fonts/Digital-Serial-Bold.ttf</fontPath>
<!--  Tamanho das letras  -->
    <fontSize>0.020</fontSize>
<!--  Posicao das letras  -->		 
	<pos>0.70 0.05</pos>
<!--  Alinhamento das letras  -->		 
    <alignment>left</alignment> 
<!--  Espaco entre as palavras  -->	
	<lineSpacing>1.5</lineSpacing> 
<!--  Barra Seletora   -->		
	<selectorImagePath></selectorImagePath>
<!--  Cor das letras  -->		 
	<primaryColor>ffffff</primaryColor>
<!--  Cor do seletor  -->		 
	<selectorColor>4682B4</selectorColor>
<!--  Cor do jogo Selecionado  -->
	<selectedColor>000000</selectedColor>	
<!--  profundidade da arte / sequencia de amostragem da arte  -->
	<zIndex>5</zIndex>	
</textlist>		 

<!------------------------------------------------------------------------------->
<!---------------------------------- HELPSYSTEM --------------------------------->
<!------------------------------------------------------------------------------->

<helpsystem name="help">
<pos>0 0.965</pos>  
</helpsystem>

